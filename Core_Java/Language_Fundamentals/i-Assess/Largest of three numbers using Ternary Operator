
import java.util.*;
public class Main{
	
	public static void main(String[] args) {
		Scanner input=new Scanner(System.in);
		System.out.println("Enter the numbers:");
		int a=input.nextInt();
		int b=input.nextInt();
		int c=input.nextInt();
		int result=(a>b && a>c)?a:(b>a && b>c)?b:c;
		System.out.println(result+" is the greatest number");
	}
	
}

